class SweetSheela
{  
	static double wallet=1500;
	public static void main(String[] args)
	{
		 System.out.println("initial amount is "+wallet);
		 double spent=56;
		 System.out.println("amount spent is "+spent);
		 wallet=wallet-spent;
		 System.out.println("available amount is "+wallet);

		
	}
}


